<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_move_inherit_field" model="ir.ui.view">
        <field name="name">account.move.inherit.field</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='tuition']" position="after">
                <field name="ten_subject_charges" string="10_Subject_Charges"/>
                <field name="It_charges" string="IT Charges"/>
            </xpath>
        </field>
    </record>
</odoo>