<odoo>
    <data>

    <template id="cus_report.dues_assesment_sheet">
          <t t-call="web.html_container"> 
                <t t-foreach="docs" t-as="o">
                  
                  <div class="row" style="">
                    
                    <div class="column" style=" border: 1px solid black; width: 100%;  height: 100%; margin-right: 1%;">

                      <table class="table table-dark" style="width:100%;">
                    <tbody>
                  <tr style="height: 65.5px;">
                  <td style="align-items: center; ">
                              <img style="padding-left:2%; " t-if="o.company_id.logo" t-att-src="image_data_uri(o.company_id.logo)" height="70" width="100" class="float-left" alt="Logo"/>
                  </td>
                  <td style="align-items: center;font-size: 10px;">
                  <p style="text-align: center; "><strong style="font-size: 25px;">LACAS School Network</strong><br/><strong style="font-size: 20px;">Dues Assessment Sheet </strong><br/></p>
                  </td>
                  <td style=" align-items: left; ">
                              <img t-if="o.company_id.image" t-att-src="image_data_uri(o.company_id.image)" height="70" width="100" class="float-left" alt="Logo"/>
                  
                  </td>
                  </tr> 
                  </tbody>
                  </table>
                    <hr style="height: 1px; border-width: 0; color: black; background-color: grey;"/>
                    
                    <div class="page">
                    <div class="row" style=" width:90%; margin: auto;  padding-top:5px;">
                    
                    <div class="col-3"/>
                    
                    <div class="col-6" style=" padding-left:10px; font:15px lucida-console,sans-serif !important;  background-color:#B2BABB;">
                    <h2>
                        <span t-if="o.move_type == 'out_invoice' and (o.state == 'draft' or o.state == 'posted')"><font color="#fff">Invoice</font></span>
                        <span t-if="o.move_type == 'out_invoice' and o.state == 'proforma2'"><font>PRO-FORMA</font></span>
                        <span t-if="o.move_type == 'out_invoice' and o.state == 'draft'"><font color="#fff">Draft Invoice</font></span>
                        <span t-if="o.move_type == 'out_invoice' and o.state == 'cancel'"><font color="#fff">Cancelled Invoice</font></span>
                        <span t-if="o.move_type == 'out_refund'"><font>Refund Receipt :</font></span>
                        <span t-if="o.move_type == 'in_refund'"><font color="#fff">Vendor Refund</font></span>
                        <span t-if="o.move_type == 'in_invoice'"><font color="#fff">Vendor Bill</font></span>
                        <font><span t-field="o.name"/></font>
                    </h2>
                    <!--<div t-if="o.invoice_date">-->
                      
                    <!--  <font color="#fff">-->
                        
                    <!--    <strong>Invoice Date:</strong><p t-field="o.invoice_date"/></font>-->
                    <!--</div>-->
                    
                    <!--<div t-if="o.name">-->
                    <!--   <font color="#fff">-->
                    <!--    <strong>Student Name:</strong> <p t-field="o.Student_id"/></font>-->
                    <!--</div>-->
                    
                    
                    
                </div>
               
              
                  
                     
                    
                     
                </div>
                </div>
                  
                   
                     
                
             <!--fields middle body -->
                
                 <div class="column" style=" width: 60%;  height: 25%; margin-right: 1%; margin-top:7%;">
                   
                    <div class="col-3" style=" width:100%; padding-left:15%;  font:15px ; ">
                  <!--<strong style="font-size:20px; " >Student Acadamics Details</strong><br/>-->
                  
                       <span style=" color:#1565C0; font-size:20px;"><strong>Branch:</strong> </span><span style=" font-size:20px; padding-left:10px;" t-field="o.school_branch"/><br/>
                      
                       <span style=" color:#1565C0; font-size:20px;"><strong>Class : </strong></span><span style=" ffont-size:20px; padding-left:10px;" t-field="o.class_grade"/><br/>
                       <span style=" color:#1565C0; font-size:20px; "><strong>Roll No: </strong></span><span style=" font-size:20px;  padding-left:10px;" t-field="o.Roll_no"/><br/>
                      <span style=" color:#1565C0; font-size:20px;"><strong>Student Name :</strong> </span><span style=" font-size:20px; padding-left:10px;" t-field="o.Student_id"/><br/>
                      <span style=" color:#1565C0; font-size:20px;"><strong>Notice Fee : </strong></span><span style=" font-size:20px; padding-left:10px;" t-field="o.school_branch"/><br/>
                    
                      
                     
                      
                      
                      <span style=" color:#1565C0; font-size:20px;"><strong>Application Submit Date :</strong> </span><span style=" font-size:20px; padding-left:10px;" t-field="o.withdrawl_submission_date"/><br/>
                      <span style=" color:#1565C0; font-size:20px;"><strong>Actual Leaving Date:</strong> </span><span style=" font-size:20px; padding-left:10px;" t-field="o.actual_leaving_date"/><br/>
                      <span style=" color:#1565C0; font-size:20px;"><strong>Notice Completion Date : </strong></span><span style=" font-size:20px; padding-left:10px;" t-field="o.notice_completion_date"/><br/>
                      <span style=" color:#1565C0; font-size:20px; width:100%; "><strong>Cheque of Security Deposit made in Favour of : </strong></span><span style=" font-size:20px; padding-left:10px; width:100%;" t-field="o.partner_id.name"/><br/>
                      
                      

                    </div>
                  </div>
                  
                  
                     
                 <table class="table table-dark" style="border-collapse:collapse;   margin: auto; width:90%;">
                  <thead>
                  <tr style="background-color: #B2BABB; width: 100px;">
                    <th style="padding-left:30px; width: 132.138px; font-size:20px;"><strong>Account</strong></th>
                    <!--<th style="text-align: right; width: 132.138px; font-size:13px;"><strong>Amount(PKR)</strong></th>-->
                    <th style="padding-Right:30px; width: 132.138px; font-size:20px;"><strong>Subtotal</strong></th>
                  </tr>
                  </thead>
                <tbody>
                      <t t-foreach="o.invoice_line_ids" t-as="d">
                      <tr style="">
                        <td style="text-align: left; border: 1px solid black; font-size:20px;"><t t-esc="d.product_id.name"/></td>
                        <!-- <td style="text-align: right; border: 1px solid black; font-size:13px;"><t t-esc="int(d.price_unit)"/></td> -->
                        <td style="text-align: right; border: 1px solid black; font-size:20px;"><t t-esc="int(d.price_subtotal)"/></td>

                      </tr>
                     
                    </t>
                    </tbody>
                  </table>
                  
                  <!--amount refundable-->
                   <table class="table table-dark" style="border-collapse:collapse;   margin: auto; width:90%;">
                     
                      <thead>
                  <tr style="background-color: #B2BABB; width: 100px;">
                    <th style="text-align: left; width: 132.138px; font-size:20px;"><strong>Total</strong></th>
                    <!--<th style="text-align: right; width: 132.138px; font-size:13px;"><strong>Amount(PKR)</strong></th>-->
                    <th style=" text-align: right;  font-size:20px;"><strong><t t-esc="o.amount_residual" t-options="{&quot;widget&quot;: &quot;monetary&quot;, &quot;display_currency&quot;: o.currency_id}"/></strong></th>
                  </tr>
                  </thead>
                  
                  <!--<thead>-->
                  <!--<tr style="background-color: #B2BABB; width: 100px;">-->
                  <!--  <th style=" width: 98px;"><strong>Amount Refundable </strong></th>-->
                    <!--<th style="text-align: right; width: 132.138px; font-size:13px;"><strong>Amount(PKR)</strong></th>-->
                  <!--  <th style="text-align: right; border: 1px solid black;"><strong><t t-esc="int(d.price_subtotal)"/></strong></th>-->
                  <!--</tr>-->
                  <!--</thead>-->
                  </table>
                  
                  <!--<table class="table table-dark" style=" padding-left:40px; width:90%;">-->
                  <!--  <thead>-->
                  <!--  <tr style="background-color: 878493; width: 100px;">-->
                  <!--  <th style=" text-decoration: underline; width: 98px; font-size:20px; margin-left: 5%;">Amount Refundable </th>-->
                  <!--  <th style="padding-Right:30px;  width: 98px; font-size:20px; margin-right: 5%;"><t t-esc="int(o.amount_residual)"/></th>-->
                  <!--  </tr>-->
                  <!--  </thead>-->
                  <!--  </table>-->
                  
                  <table class="table " style="width:100%; padding-top: 25%;">
                      <tbody>
                       <td style="width:20%"> 
                       <hr style="height: 1px; border-width: 0; padding-right:10px; color: black; background-color: black;"/>
                       <span style=" padding-left:20%; font-size:20px;"><strong>Prepared By </strong></span>  
                      <!--<SPAN STYLE="text-decoration:overline;  text-align: left;  font-size: 14px; font-weight:bold">Bank Officer Sign., Stamp  Date</SPAN>-->
                    </td>
                    
                     <td style="width:10%">
                    </td>
                    
                    <td style="width:20%"> 
                       <hr style="height: 1px; border-width: 0; color: black; background-color: black;"/>
                       <span style="padding-left:30%; font-size:20px;"><strong>Checked By </strong></span>  
                      <!--<SPAN STYLE="text-decoration:overline;  text-align: left;  font-size: 14px; font-weight:bold">Bank Officer Sign., Stamp  Date</SPAN>-->
                    </td>
                    
                     <td style="width:10%">
                    </td>
                   
                    
                    <td style="width:20%">
                         <hr style=" padding-right:10px; height: 1px; border-width: 0; color: black; background-color: black;"/>
                       <span style=" padding-left:25%; font-size:18px; "><strong>Approved by</strong></span>  
                      <!--<SPAN STYLE="text-decoration:overline; text-align: right;  font-size: 14px; font-weight:bold"> Sign. Depositor</SPAN> -->
                    </td>
                    <td style="width:3%">
                    </td>
                    </tbody>    
                    </table>
                    
                    <!-- agreement-->
                    
                     <hr style="height: 1px; border-width: 3px; background-color: red;"/>
                     
                     <h3 style="text-align:center; font-size:18px; ">Receiving</h3>
                     
                     <p style="font-size:18px; padding-left:40px; padding-right:40px;">
                       I, <strong> <t t-esc="o.Student_id"/></strong>, do hereby confirm that I have received my entire dues from LACAS vide
Cheque # Dated<strong> <t t-esc="o.invoice_date"/></strong> Rs.150000 and I have no claim against School.
                     </p><br/>
                     
                     <table class="table " style="width:100%; padding-top: 20%;">
                      <tbody>
                       <td style="width:20%"> 
                       
                    </td>
                    
                     <td style="width:10%">
                    </td>
                    
                  
                   
                   
                    
                    <td style="width:7% ;" >
                         <hr style="height: 1px; border-width: 0; color: black; margin-right:30px; background-color: black;"/>
                       <span style="padding-left:40%; font-size:18px;"><strong>Student</strong></span>  
                      <!--<SPAN STYLE="text-decoration:overline; text-align: right;  font-size: 14px; font-weight:bold"> Sign. Depositor</SPAN> -->
                    </td>
                     
                    <!-- <td style="width:2%">-->
                    <!--</td>-->
                    </tbody>    
                    </table>
                   

                   </div>
                    
                   </div>
                  
                  
                   
                </t>
            </t>
        </template>



    </data>
</odoo>