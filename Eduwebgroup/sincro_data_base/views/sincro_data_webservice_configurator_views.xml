<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sincro_data_base_webservice_configurator_view_form" model="ir.ui.view">
        <field name="name">sincro_data_base.webservice_configurator.view.form</field>
        <field name="model">sincro_data_base.webservice_configurator</field>
        <field name="arch" type="xml">
            <form string="Application Programming Interface">
                <sheet>
                    <group>
                        <group colspan="1">
                            <field name="name"/>
                            <field name="label"/>
                            <field name="panel_configuration"/>
                            <field name="pretty"/>
                        </group>
                        <group colspan="1">
                            <field name="model_id"/>
                            <field name="model_name" invisible="1"/>
                            <field name="domain" widget="domain" options="{'model': 'model_name', 'in_dialog': True}"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="sincro_data_base_webservice_configurator_view_tree" model="ir.ui.view">
        <field name="name">sincro_data_base.webservice_configurator.view.tree</field>
        <field name="model">sincro_data_base.webservice_configurator</field>
        <field name="arch" type="xml">
            <tree string="Webservice configurator">
                <field name="name"/>
                <field name="label"/>
                <field name="panel_configuration"/>
                <field name="model_id"/>
                <field name="model_name"/>
                <field name="domain"/>
                <field name="pretty"/>
            </tree>
        </field>
    </record>

</odoo>
